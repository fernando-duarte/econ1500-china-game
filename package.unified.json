{
  "name": "china-growth-game-unified",
  "version": "1.0.0",
  "description": "Unified implementation of China's Growth Game for economic education",
  "main": "unified-server.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node unified-server.js",
    "dev": "nodemon unified-server.js",
    "start:react": "cd frontend && npm run start",
    "build:react": "cd frontend && npm run build",
    "dev:all": "concurrently \"npm run dev\" \"npm run start:react\" \"npm run start:model\"",
    "start:model": "cd economic-model && uvicorn app:app --reload --port 8000",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "aws-sdk": "2.1580.0",
    "axios": "1.8.4",
    "body-parser": "2.2.0",
    "cookie": "0.7.0",
    "cors": "2.8.5",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "node-vault": "0.10.2",
    "path-to-regexp": "8.2.0",
    "socket.io": "4.7.5"
  },
  "devDependencies": {
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "nodemon": "3.1.9",
    "socket.io-client": "4.7.5"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/china-growth-game.git"
  },
  "keywords": [
    "economics",
    "education",
    "game",
    "simulation",
    "growth",
    "china"
  ],
  "author": "Your Name",
  "license": "MIT"
} 